    <application

    android:usesCleartextTraffic="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher"
    android:theme="@style/AppTheme">




    <activity
        android:networkSecurityConfig="@xml/network_security_config"
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar"
        android:windowSoftInputMode="stateAlwaysHidden">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>





    <activity
        android:name=".Inicio"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/app_name"

        >
    </activity>




    <activity
        android:name=".Searchs"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="Busquedas"

        >
    </activity>



        <activity
            android:name=".CategoriasMarcas"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Categorias"

            >
        </activity>



        <activity
        android:name=".LIBS.Login"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/app_name"

        >
    </activity>








    <activity
        android:name=".Buscador"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:focusableInTouchMode="true"
        android:windowSoftInputMode="stateHidden"
        >
    </activity>







    <activity
        android:name=".Delivery"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:focusableInTouchMode="true"
        android:windowSoftInputMode="stateHidden"
        >
    </activity>




    <activity
        android:name=".Details"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        >
    </activity>






        <activity
            android:name=".Herramientas_Ventas"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            >
        </activity>



    <activity
        android:name=".Web_View_General"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar"
        android:label="Mis Pedidos"
        >
    </activity>




    <meta-data
        android:name="com.facebook.sdk.ApplicationId"
        android:value="@string/facebook_app_id"/>

    <activity
        android:name="com.facebook.FacebookActivity"
        android:configChanges= "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
        android:label="@string/app_name" />


    <activity android:name="com.facebook.CustomTabActivity" android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.VIEW" />
            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />
         <data android:scheme="@string/fb_login_protocol_scheme" /> </intent-filter>
    </activity>


